<?xml version="1.0" encoding="utf-8" ?>
<Theme>

  <Style Name="Custom" Type="Button" State="Normal">
    <Fill>
      <LinearGradient Orientation="Vertical">
        <GradientStop Stop="0" Color="Blue"/>
        <GradientStop Stop="1" Color="Green"/>
      </LinearGradient>
    </Fill>
    <Stroke>
      <Solid Color="Gray"/>
    </Stroke>
  </Style>
  
  <Style Name="Default" Type="Button" State="Normal">
    <Fill>
      <LinearGradient Orientation="Vertical">
        <GradientStop Stop="0" Color="Honeydew2"/>
        <GradientStop Stop="1" Color="Honeydew3"/>
      </LinearGradient>
    </Fill>
    <Stroke>
      <Solid Color="Gray"/>
    </Stroke>
  </Style>

  <Style Name="Default" Type="Button" State="Over">
    <Fill>
      <LinearGradient Orientation="Vertical">
        <GradientStop Stop="0" Color="Honeydew2"/>
        <GradientStop Stop="1" Color="LightGray"/>
      </LinearGradient>
    </Fill>
    <Stroke>
      <Solid Color="Gray"/>
    </Stroke>
  </Style>

  <Style Name="Default" Type="Button" State="Down">
    <Fill>
      <LinearGradient Orientation="Vertical">
        <GradientStop Stop="0" Color="Honeydew3"/>
        <GradientStop Stop="1" Color="Honeydew4"/>
      </LinearGradient>
    </Fill>
    <Stroke>
      <Solid Color="Gray"/>
    </Stroke>
  </Style>

  <Style Name="Default" Type="Button" State="Active">
    <Stroke>
      <Location L="3px" T="3px" R="3px" B="3px"/>
      <Rectangle/>
      <Dash Color="DarkGray" Size="3px"/>
    </Stroke>
  </Style>

  <Style Name="Default" Type="Frame" State="Normal">
    <Fill>
      <Solid Color="1,0.9,0.9,0.9"/>
    </Fill>
    <Stroke>
      <Solid Color="Gray"/>
    </Stroke>
  </Style>

  <Style Name="Default" Type="Window" State="Normal">
    <Fill>
      <Solid Color="White"/>
    </Fill>
  </Style>
</Theme>